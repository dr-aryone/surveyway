  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^1.3.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.8",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "react-scripts": "^1.0.13",
    "stripe": "^5.1.0"
  }
}
