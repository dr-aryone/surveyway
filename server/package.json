{
  "name": "emaily-app",
  "version": "1.0.0",
  "description": "An Email Survey App",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "yarn": "1.0.1"
  },
  "scripts": {
    "heroku-postbuild": "cd client && yarn --production=false && yarn run build",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
